.SUFFIXES: .cpp

srcdir = .

top_builddir    = ..
top_srcdir      = ..

SHELL = /bin/sh



CXX = cl
CXXFLAGS = /nologo /G6 /MT /W3 /GX- /DNDEBUG /DWIN32 /D_WINDOWS /Ox 

LDFLAGS = 
LIBS = 
X_LIBS = 
X_PRE_LIBS = 
RANLIB = echo


BX_INCDIRS = -I.. -I$(srcdir)/.. -I../instrument/stubs -I$(srcdir)/../instrument/stubs

BX_OBJS = \
	memory.o misc_mem.o

BX_INCLUDES = ../bochs.h ../config.h

all: libmemory.a

.cpp.o:
	$(CXX) /c $(BX_INCDIRS) $(CXXFLAGS) /Tp$< /Fo$@


libmemory.a: $(BX_OBJS)
	-del libmemory.a
	lib /nologo /subsystem:console /machine:I386 /verbose /out:$@ $(BX_OBJS)
	$(RANLIB) libmemory.a

$(BX_OBJS): $(BX_INCLUDES)

clean:
	-del *.o
	-del *.a

dist-clean: clean
	-del Makefile

